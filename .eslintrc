{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": 0,
    "space-before-function-paren": 0,
    "no-shadow": 0,
    "consistent-return": 0,
    "prefer-template": 0,
    "prefer-arrow-callback": 0,
    "no-var": 0,
    "arrow-parens": 0,
    "no-else-return": 0,
    "class-methods-use-this": 0
  },
  "extends": ["airbnb-base"]
}